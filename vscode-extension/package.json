{
  "name": "clj-kondo",
  "description": "Clj-kondo language server and client bundled in one extension.",
  "author": "Michiel Borkent",
  "license": "EPL-1",
  "version": "2020.7.29",
  "publisher": "borkdude",
  "repository": {
    "type": "git",
    "url": "https://github.com/borkdude/clj-kondo.lsp"
  },
  "engines": {
    "vscode": "^1.33.0"
  },
  "main": "./out/extension.js",
  "activationEvents": [
    "onLanguage:clojure"
  ],
  "icon": "icon.png",
  "scripts": {
    "vscode:prepublish": "npm run update-vscode && npm run compile",
    "compile": "tsc -b",
    "watch": "tsc -b -w",
    "update-vscode": "vscode-install",
    "postinstall": "vscode-install"
  },
  "dependencies": {
    "vscode-languageclient": "^5.2.1"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.0",
    "@types/node": "^8.0.0",
    "tslint": "^5.16.0",
    "typescript": "^3.6.4",
    "vscode": "^1.1.35"
  },
  "contributes": {
    "configuration": {
      "title": "clj-kondo",
      "properties": {
        "clj-kondo.config.filePath": {
          "type": "string",
          "scope": "application",
          "markdownDescription": "Specifies file argument or EDN arguement to merge with config.edn."
        }
      }
    }
  }
}
